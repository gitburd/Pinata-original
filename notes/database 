this should work if you take the 1st 100 lines, there won't be any records
CREATE DATABASE pinata;
\connect pinata;

CREATE TABLE users (user_id SERIAL PRIMARY KEY, auth0_id VARCH, user_name VARCHAR, first_name VARCHAR, last_name VARCHAR, email VARCHAR, user_name VARCHAR);

CREATE TABLE skills (skill_id SERIAL PRIMARY KEY, skill_title VARCHAR, skill_details VARCHAR, skill_icon VARCHAR, is_critical BOOLEAN, user_id VARCHAR );

CREATE TABLE emotions (emotion_id SERIAL PRIMARY KEY, emotion_text VARCHAR);

CREATE TABLE emotion_skills (emotion_skills_id SERIAL PRIMARY KEY, skill_id INT , emotion_id INT, is_star boolean);

CREATE TABLE records (record_id SERIAL PRIMARY KEY, user_id INT, skill_id VARCHAR, emotion_id INT, before_lvl INT, after_lvl INT, impact INT, date DATE, si BOOLEAN, sh BOOLEAN);


update emotion_skills set is_star = true WHERE emotion_skills_id>0;



